{
  "configurations": [
    {
      "name": "linux-g++-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include/c++/11",
        "/usr/include/x86_64-linux-gnu/c++/11",
        "/usr/include/**",
        "/usr/local/include/**",
        "/usr/include/opencv4"
      ],
      "defines": [
        "__MAJOR__=0",
        "__MINOR__=0",
        "__REV__=1"
      ],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "configurationProvider": "ms-vscode.makefile-tools",
      "C_Cpp.intelliSenseEngine": "default",
      "C_Cpp.autocomplete": "default"
    }
  ],
  "version": 4
}